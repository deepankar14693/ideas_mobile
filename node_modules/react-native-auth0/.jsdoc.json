{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "auth",
            "management",
            "networking",
            "utils",
            "webauth",
            "index.js",
            "package.json",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|__tests__)"
    },
    "plugins": [
        "./node_modules/jsdoc/plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "opts": {
        "destination": "./out/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}